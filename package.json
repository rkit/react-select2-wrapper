    "react-dom": "^0.14.1",
    "select2": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.4",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "^0.0.8",
    "eslint-plugin-react": "^3.2.0",
    "extract-text-webpack-plugin": "^0.7.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
